<html>
  <head>
    <title>ÖTSV Veranstaltungen 2016</title>
    <link rel="stylesheet" type="text/css" href="app.css" />
    <style>
    a { text-decoration:none }
    .register { background-color:#ffff80 }
    </style>
    <script src="js/common.js"></script>
    <macro show_serie(serie, not_series, ignore)>
    <p>
      $ var events = data.events(serie, not_series);
      <for(event in events)>
	<a href="event/${event.id}/results">${event.title}</a> ${data.abbreviations(event.series, ignore=ignore)}<br>
      </for>
    </p>
    </macro>
  </head>
  <body>
    <h1>ÖTSV Veranstaltungen 2016</h1>
    <h2>ÖTSV Cup und AMF Staatsmeisterschaft,<br>ÖTSV Kids e-Cup,<br>ÖTSV Bike Trial Cup</h2>
    <show_serie serie=24 ignore=[24]/>
    <p>
      <a href="serie/25/results">ÖTSV Cup Jahreswertung</a><br>
      <a href="serie/26/results">AMF Staatsmeisterschaft Jahreswertung</a><br>
      <a href="serie/29/results">Kids e-Cup Jahreswertung</a><br>
      <a href="serie/27/results">Bike Trial Cup Jahreswertung</a>
    </p>
  </body>
</html>
