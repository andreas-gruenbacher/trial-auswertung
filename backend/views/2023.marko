<html>
  <head>
    <title>ÖTSV Veranstaltungen 2023</title>
    <link rel="stylesheet" type="text/css" href="app.css" />
    <style>
    a { text-decoration:none }
    .register { background-color:#ffff80 }
    </style>
    <script src="js/common.js"></script>
    <macro show_serie(serie, not_series, ignore)>
    <p>
      $ var events = data.events(serie, not_series);
      <for(event in events)>
	<a href="event/${event.id}/results">${event.title}</a> ${data.abbreviations(event.series, ignore=ignore)}<br>
      </for>
    </p>
    </macro>
  </head>
  <body>
    <h1>ÖTSV Veranstaltungen 2023</h1>
    <h2>ÖTSV Cup und AMF Staatsmeisterschaft</h2>
    <show_serie serie=69 ignore=[68,74]/>
    <p>
      <a href="serie/69/results">ÖTSV Cup + Damencup Jahreswertung</a><br>
      <a href="serie/70/results">AMF Staatsmeisterschaft Jahreswertung</a>
    </p>

    <h2>ÖTSV A-Cup</h2>
    <show_serie serie=71 ignore=[68,71]/>
    <p>
      <a href="serie/71/results">Jahreswertung</a>
    </p>

    <h2>ÖTSV e-Kids Cup</h2>
    <show_serie serie=72 ignore=[68,72]/>
    <p>
      <a href="serie/72/results">Jahreswertung</a>
    </p>

    <h2>ÖTSV Bike Trial Cup</h2>
    <show_serie serie=73 ignore=[68,73]/>
    <p>
      <a href="serie/73/results">Jahreswertung</a>
    </p>
  </body>
</html>
